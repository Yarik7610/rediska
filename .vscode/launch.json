  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Master",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "args": ["-port", "6380", "-dir", ".", "-dbfilename", "dump.rdb"]
    },
    {
      "name": "Launch Replica",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${fileDirname}",
      "args": ["-port", "6381", "-replicaof", "127.0.0.1 6380"]
    }
  ]
}
